      </c40:InTextReferencePointer>
    </po:contains>

    <po:contains>
      <c40:InTextReferencePointer rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_12_reference2">
        <c40:denotes rdf:resource="https://sakedhdk.github.io/knowledge_graph#bibliography_9"/>
        <c40:hasContext rdf:resource="https://sakedhdk.github.io/knowledge_graph#sentence_12"/>
        <c40:hasContent>McLean &amp; Anderson, 1997</c40:hasContent>
      </c40:InTextReferencePointer>
    </po:contains>

    <po:contains>
      <sake:InResourcePointer rdf:about="https://sakedhdk.github.io/knowledge_graph#figure_1_reference_sentence_12">
