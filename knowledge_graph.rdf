  <rdf:Description rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_50">
    <rdf:type rdf:resource="http://purl.org/spar/doco/Sentence"/>
    <c40:hasContent>The duration of head impacts in National Football League players who suffer concussion is typically 15ms (Pellman et al., 2003).</c40:hasContent>
    <po:contains rdf:resource="https://sakedhdk.github.io/knowledge_graph#sentence_50_reference1"/>
  </rdf:Description>

