                  <rdf:Description rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_18">
                    <rdf:type rdf:resource="http://purl.org/spar/doco/Sentence"/>
                    <c40:hasContent>This analysis appears to miss the dynamic effect: the brain itself accelerates during the impact.</c40:hasContent>
                  </rdf:Description>
                </po:contains>

                <po:contains rdf:resource="https://sakedhdk.github.io/knowledge_graph#sentence_19"/>
              </rdf:Description>
            </po:contains>

