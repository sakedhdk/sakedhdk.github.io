  <rdf:Description rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_51">
    <rdf:type rdf:resource="http://purl.org/spar/doco/Sentence"/>
    <c40:hasContent>For this range of duration of human head impact (3–15 ms), the tolerable acceleration ranges from 80 g to 160 g.</c40:hasContent>
  </rdf:Description>

  <rdf:Description rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_52">
    <rdf:type rdf:resource="http://purl.org/spar/doco/Sentence"/>
    <c40:hasContent>The duration of impact for the acorn woodpecker is remarkably short: 0.5–1.0 ms (May et al., 1979).</c40:hasContent>
    <po:contains rdf:resource="https://sakedhdk.github.io/knowledge_graph#sentence_52_reference1"/>
  </rdf:Description>

  <c40:InTextReferencePointer rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_52_reference1">
