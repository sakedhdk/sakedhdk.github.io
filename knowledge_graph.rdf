      </c40:InTextReferencePointer>
    </po:contains>

    <po:contains rdf:resource="https://sakedhdk.github.io/knowledge_graph#sentence_20_reference4"/>
  </rdf:Description>

  <c40:InTextReferencePointer rdf:about="https://sakedhdk.github.io/knowledge_graph#sentence_22_reference4">
    <c40:denotes rdf:resource="https://sakedhdk.github.io/knowledge_graph#bibliography_7"/>
